<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".KimsinActivity">

    <!-- Yönetici Butonu -->
    <Button
        android:id="@+id/btnYonetici"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Yöneticiysen Bu Butona Tıkla"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/btnUye" />

    <!-- Üye Butonu -->
    <Button
        android:id="@+id/btnUye"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Üye isen Bu Butona Tıkla"
        app:layout_constraintTop_toBottomOf="@+id/btnYonetici"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
